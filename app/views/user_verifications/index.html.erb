<h1>Listing user_verifications</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Birthday</th>
      <th>Is intern</th>
      <th>Mobile</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Department</th>
      <th>Position</th>
      <th>Area</th>
      <th>Subjection</th>
      <th>Tel</th>
      <th>Fax</th>
      <th>Im</th>
      <th>Id card</th>
      <th>Adress</th>
      <th>Postcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_verifications.each do |user_verification| %>
      <tr>
        <td><%= user_verification.name %></td>
        <td><%= user_verification.birthday %></td>
        <td><%= user_verification.is_intern %></td>
        <td><%= user_verification.mobile %></td>
        <td><%= user_verification.gender %></td>
        <td><%= user_verification.email %></td>
        <td><%= user_verification.department %></td>
        <td><%= user_verification.position %></td>
        <td><%= user_verification.area %></td>
        <td><%= user_verification.subjection %></td>
        <td><%= user_verification.tel %></td>
        <td><%= user_verification.fax %></td>
        <td><%= user_verification.im %></td>
        <td><%= user_verification.id_card %></td>
        <td><%= user_verification.adress %></td>
        <td><%= user_verification.postcode %></td>
        <td><%= link_to 'Show', user_verification %></td>
        <td><%= link_to 'Edit', edit_user_verification_path(user_verification) %></td>
        <td><%= link_to 'Destroy', user_verification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User verification', new_user_verification_path %>
